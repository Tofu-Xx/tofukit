{
  "name": "@tofukit/resetcss",
  "type": "commonjs",
  "version": "0.1.3",
  "description": "You don't want to reset your css every time",
  "author": "tofu-xx <tofu-xx@foxmail.com>",
  "license": "MIT",
  "keywords": [
    "css"
  ],
  "exports": {
    ".": {
      "import": "./dist/reset.css"
    }
  },
  "main": "dist/reset.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "release": "bumpp --no-push && nr build && pnpm publish",
    "build": "tsup"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "postcss-preset-env": "^10.0.2",
    "tsup": "^8.2.4"
  }
}
